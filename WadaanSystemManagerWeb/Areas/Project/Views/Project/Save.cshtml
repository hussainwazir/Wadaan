
@{
    ViewBag.Title = "Save";
    //Layout = "~/Areas/Employee/Views/Shared/_EmployeeLayout.cshtml";
    Layout = "~/Areas/Shared/_EmployeeLayout.cshtml";
}

<body>

    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>

    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full"
         data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">

        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb">
                <div class="row">
                    <div class="col-12 d-flex no-block align-items-center">

                        <div class="ms-auto text-end">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="/Employee/Employee/Dashboard"> Dashboard </a></li>
                                    <li class="breadcrumb-item active" aria-current="page"> Project </li>
                                    <li class="breadcrumb-item active" aria-current="page"> Save</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div class="card">

                    <div class="card-header"><h5>Project Registration Form</h5>  </div>
                    <div class="card-body">

                        <form class="md-float-material" id="frmAddUpdateProject" name="frmAddUpdateProject" action="/services/Xtreme/multipart" method="post">
                            <input type="hidden" name="UserID" id="UserID" value="00000000-0000-0000-0000-000000000000">
                            <input type="hidden" name="PlotSizename" id="PlotSizename" value="">
                            <input type="hidden" name="categoryTypeName" id="categoryTypeName" value="">
                            <input type="hidden" name="ProjectID" id="ProjectID" value="00000000-0000-0000-0000-000000000000">
                            <input type="hidden" name="Id" id="Id" value="0">
                            <input type="hidden" name="type" id="type" value="Project_Save">


                            @* =========================== PROJECT INFORMAION =========================== *@

                            <div class="card">
                                <div class="card-header"> <h5> Project Information</h5> </div>
                                <div class="card-body border">

                                    <div class="row">
                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Project Name:<span style="color:red">*</span></label>
                                                <input type="text" class="form-control pname" name="ProjectName" id="ProjectName" required value="">

                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Client Name:<span style="color:red">*</span></label><br />
                                                <select class="form-control select2" id="ClientID" required name="ClientID" tabindex="2"></select>
                                            </div>
                                        </div>


                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Priority:</label><br />
                                                <select class="form-control select2" id="Priority" name="Priority"></select>
                                            </div>
                                        </div>
                                        

                                    </div>
                                    <br />

                                   
                                    <br />

                                    <div class="row">
                                       

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Site Plane:</label>
                                                <input type="file" multiple="true" class="form-control pname" name="ProjectAttachements" id="ProjectAttachements" value="" required>
                                            </div>
                                        </div>



                                    </div>
                                </div>

                            </div>


                            <div class="card">
                                <div class="card-header"> <h5> Site Information </h5> </div>
                                <div class="card-body border">

                                    <div class="row">

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">GPS Cordinates:</label><br />
                                                <input type="text" class="form-control pname" name="GPSCordinates" id="GPSCordinates" value="">
                                            </div>
                                        </div>

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Google Location:</label><br />
                                                <input type="text" class="form-control pname" name="GoogleLocation" id="GoogleLocation" value="" >
                                            </div>
                                        </div>

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">East:</label><br />
                                                <select class="form-control select2" id="East" name="East"></select>
                                            </div>
                                        </div>

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">West:</label><br />
                                                <select class="form-control select2" id="West" name="West"></select>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">North:</label><br />
                                                <select class="form-control select2" id="North" name="North"></select>
                                            </div>
                                        </div>

                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">South:</label><br />
                                                <select class="form-control select2" id="South" name="South"></select>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>




                            <div class="card">
                                <div class="card-header"> <h5> Contract Type </h5> </div>
                                <div class="card-body border">
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Contract Type:<span style="color:red">*</span></label><br />
                                                <select class="form-control select2" id="ContractType" required name="ContractType"></select>
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div>
                                                <label class="form-control-label">Project Category:<span style="color:red">*</span></label><br />
                                                <select class="form-control select2" id="ProjectCategory" required name="ProjectCategory" onchange="ShowHideControls()"></select>
                                            </div>
                                        </div>
                                        <div class="col-sm-3" id="div-residential">
                                            <div>
                                                <label class="form-control-label"> Plot Size:<span style="color:red">*</span></label><br />
                                                <select class="form-control select2" style="width:250px" onchange="GetProjectTimelineEstimationResidential()"  id="ResidentialPlotSize" name="ResidentialPlotSize"></select>
                                            </div>
                                        </div>

                                        <div class="col-sm-3" id="div-commercial">
                                            <div>
                                                <label class="form-control-label"> Plot Size:<span style="color:red">*</span></label><br />
                                                <select class="form-control select2" id="CommercialPlotSize" onchange="GetProjectTimelineEstimationCommercial()" required style="width:250px" name="CommercialPlotSize"></select>
                                            </div>
                                        </div>


                                        <div class="col-sm-3" style="margin-top:17px;font-weight:bold;">
                                            <div>
                                                <label class="form-control-label"> Total Estimation: </label><br />
                                                <span id="lbl-project-time-line-estimation" style="color:red"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            






                                    <div class="head">
                                        <div class="card-header"> <h5> Plot Information</h5> </div>
                                        <div class=" card-body border">

                                            <div class="row">

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Contractor Name:</label><br />
                                                        <select class="form-control select2" id="ContractorID" name="ContractorID"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Construction Status:</label>
                                                        <input type="text" class="form-control pname" name="ConstructionStatus" id="ConstructionStatus" value="">

                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Start Date:</label><br />
                                                        <input type="text" class="form-control pname" name="StartDate" id="StartDate" value="">
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Sub Structure:</label><br />
                                                        <select class="form-control select2" id="SubStructure" name="SubStructure"></select>
                                                    </div>
                                                </div>

                                            </div>
                                            <br />

                                            <div class="row">

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Super Structure:</label><br />
                                                        <select class="form-control select2" id="SuperStructure" name="SuperStructure"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Finishing:</label><br />
                                                        <select class="form-control select2" id="Finishing" name="Finishing"></select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Plot House Number:</label><br />
                                                        <input type="text" class="form-control pname" name="PlotHouseNo" id="PlotHouseNo" value="">
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Street:</label><br />
                                                        <input type="text" class="form-control pname" name="Street" id="Street" value="">
                                                    </div>
                                                </div>

                                                @*<div class="col-sm-3" id="div-residential">
            <div>
                <label class="form-control-label">Residential Plot Size:<span style="color:red">*</span></label><br />
                <select class="form-control select2" id="ResidentialPlotSize" name="ResidentialPlotSize"></select>
            </div>
        </div>

        <div class="col-sm-3" id="div-commercial">
            <div>
                <label class="form-control-label">Commercial Plot Size:<span style="color:red">*</span></label><br />
                <select class="form-control select2" id="CommercialPlotSize" name="CommercialPlotSize"></select>
            </div>
        </div>*@


                                            </div>
                                            <br />
                                            <div class="row">


                                               
                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Town / Sector:</label><br />
                                                        <input type="text" class="form-control pname" name="TownSector" id="TownSector" value="">
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">City:</label><br />
                                                        <select class="form-control select2" id="CityID" name="CityID"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Province:</label><br />
                                                        <select class="form-control select2" id="ProvinceID" name="ProvinceID"></select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <br />

                                    <div class="head" style="display:none">
                                        <div class="card-header"> <h5> Drawing Information </h5> </div>
                                        <div class="card-body border">

                                            <div class="row">

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Sketch:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="Sketch" name="Sketch"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Submission Drawing:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="SubmissionDrawing" name="SubmissionDrawing"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Architectural Drawing:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="ArchitecturalDrawing" name="ArchitecturalDrawing"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Structural Drawing:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="StructuralDrawing" name="StructuralDrawing"></select>
                                                    </div>
                                                </div>

                                            </div>
                                            <br />

                                            <div class="row">

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Electrical Drawing:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="ElectricalDrawing" name="ElectricalDrawing"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Plumbing Drawing:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="PlumbingDrawing" name="PlumbingDrawing"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Estimation:<span style="color:red"></span></label><br />
                                                        <select class="form-control select2" id="Estimation" name="Estimation"></select>
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div>
                                                        <label class="form-control-label">Type Of Supervision Services:<span style="color:red">*</span></label><br />
                                                        <input type="text" class="form-control pname" name="TypeOfSupervisionServices" id="TypeOfSupervisionServices" value="">
                                                    </div>
                                                </div>

                                            </div>
                                            <br />


                                        </div>
                                    </div>


                        </form>

                        <div class="border-top">

                            <div class="modal-footer">
                                <button type="button" id="btnSave" class="btn btn-success text-white   waves-effect  f-w-600 d-inline-block save_btn"> Save</button>
                                <button type="reset" id="btnReset" class="btn btn-danger text-white   waves-effect  f-w-600 d-inline-block reset_btn"> Reset</button>
                                @*<button type="reset" class="btn btn-danger text-white">Reset</button>*@

                            </div>
                        </div>


                    </div>
                </div>
            </div>




        </div>
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->
    </div>

     
</body>



<script src="~/Scripts/FunctionalityJs/ProjectArea/Project/project-save.js?wadaanv=5.1.9"></script>

